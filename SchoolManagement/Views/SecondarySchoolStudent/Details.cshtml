@section Scripts {
    <script>
        $(document).ready(function () {
            var id = $("#PrimarySchoolStudentID").val();

        });


        $("#successMessage").dialog({
            show: { effect: 'bounce', duration: 350, /* SPECIF ARGUMENT */ times: 8 },
            modal: true,
            height: 200,
            width: 300,

        });//.css("background-color", "grey");

        var countDownVal = 1;
        if ('@ViewBag.Success' != "") {
            var timer1 = setInterval(function () { myTimer() }, 20000);
            function myTimer() {
                countDownVal--;
                $('#dCountDown').html(countDownVal);
                if (countDownVal == 0) {
                    clearInterval(timer1);
                    $(window).unbind('beforeunload');
                    //window.location.href = "/Account/Login";
                    window.location.href = "/Home/Index";
                    //   $("#form").submit();

                };
            };
        }


    </script>
